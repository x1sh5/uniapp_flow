.sc-button {
		display: inline-block;
		text-align: center;
		margin-top: auto;
		margin-bottom: auto;
		border: 0px solid #6c4ad1;
		border-radius: 20rpx;
		/* line-height 应与height一致才能使文字竖直居中 */
		line-height: 70rpx;
		margin: 5rpx;
		height: 70rpx;
		width: 100px;
}
.text-area {
		display: flex;
		justify-content: center;
}
.title {
		font-size: 36rpx;
		color: #8f8f94;
}
.uni-mt-10 {
		margin-top: 10px;
		display: block;
}
.swiper {
		height: 300rpx;
}
.swiper-item {
		display: block;
		height: 300rpx;
		line-height: 300rpx;
		text-align: center;
}
.uni-bg-red {
		background-color: red;
}
.uni-bg-green {
		background-color: green;
}
.uni-bg-blue {
		background-color: blue;
}
.totop {
		display: flex;
		flex-direction: column;
		position: fixed;
		right: 0rpx;
		bottom: 70px;
		width: 60rpx;
		height: 60rpx;
		z-index: 99;
		color:  #4d1ae4;
		
		/*box-shadow: 0px 5px 2px rgba(116, 116, 116, 0.5);*/
		border-radius: 80rpx;
}
.topicon {
		width: 80rpx;color:  #4d1ae4;
		height: 60rpx; margin-left: -20rpx;margin-top: -20rpx;
		background-image: url("data:image/png;base64,");
}
.r {
		
	
		border: 10px solid #6c4ad1;
		border-radius: 0rpx;
		/*设置按钮边框为圆角 */
		border: none;
	
		width: 700rpx;
		height: 80rpx;
	
		color: rgb(255, 255, 255);
		background-color: #4d1ae4;
		font-size: 30rpx;
		text-align: center;
		/* top: calc(150vh - 350rpx);
	   */
	
	
		margin-left: -120px;
		margin-top: 0rpx;
		box-shadow: 0ch;
		padding-left: 400rpx;
		box-shadow: 0px 5px 2px rgba(116, 116, 116, 0.5);
		background-image: -webkit-linear-gradient(0deg, #4d1ae4 0%, #886cdb 100%);
}
.rowlayout{
  display: flex;
  flex-direction: row;
}
.columnlayout{
  display: flex;
  flex-direction: column;height: 200rpx;
  transform: skewX(-15deg); /* 在水平方向上倾斜 -15 度 */
}
.bluecolor{
  color: blue;
}
.blackcolor{
  color: black;
}
.italic{
  font-size: large;
  font-style: italic;
}
.custom-margin{
	margin:5px 10px;
}
.user-avatar{
	width: 48px;
	height: 48px;
    display: block;
    position: relative;
    background-size: cover;
    border-radius: 50%;
    margin: 0;
    padding: 0;
}
.user-avatar-img{
	border-radius: 50%;
	border: none;
	display: block;
	width: 48px;
	height: 48px;
	object-fit: cover;
	image-rendering: -webkit-optimize-contrast;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 100%;
    height: 100%;
}
.scroll-Y {
		height: 300rpx;
}
.scroll-view_H {
		white-space: nowrap;
		width: 100%;
}
.scroll-view-item {
		height: 300rpx;
		line-height: 300rpx;
		text-align: center;
		font-size: 36rpx;
}
.scroll-view-item_H {
		display: inline-block;
		width: 100%;
		height: 300rpx;
		line-height: 300rpx;
		text-align: center;
		font-size: 36rpx;
}
.select {
		color: #fb6583;
}
